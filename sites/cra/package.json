    "react": "16.14.0",
    "react-dev-inspector": "1.6.0",
    "react-dom": "16.14.0",
    "react-github-corner": "2.5.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "11.2.0",
